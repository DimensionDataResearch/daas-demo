{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Provisioning": {
    "Images": {
      "SQL": "microsoft/mssql-server-linux:2017-CU1",
      "SQLExporter": "tintoy.azurecr.io/daas/prometheus-mssql-exporter:1.0.0-dev"
    }
  },
  "Prometheus": {
    "Enable": true,
    "ApiEndPoint": "http://kr-cluster.tintoy.io:30900/"
  },
  "SQL": {
    "ApiEndPoint": "http://kr-cluster.tintoy.io:31250/"
  },
  "Kubernetes": {
    "ClusterPublicFQDN": "kr-cluster.tintoy.io",
    "VolumeClaimName": "sql-data-claim"
  }
}
